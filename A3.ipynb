    "from zipfile import ZipFile\n",
    "\n",
    "# Load the zip file and create a zip object\n",
    "with ZipFile(\"./faces.zip\", 'r') as zip_ref:\n",
    "    # Extract all the files present in the zip to a specific location\n",
    "    zip_ref.extractall(\"./faces\")"
